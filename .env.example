# docker-local-llm-agent

# Example environment variables for docker-local-llm-agent
# Copy this file to .env and fill in your values

# Model Runner configuration
MODEL_BASE_URL=http://localhost:12434/engines/llama.cpp/v1
MODEL_NAME=ai/gemma3:latest

# Tavily Web Search
TAVILY_API_KEY=your_tavily_api_key

# Redis configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_USERNAME=default
REDIS_PASSWORD=

# Postgres configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5433
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=agentdb

# App configuration
PORT=3001
